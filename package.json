  },
  "homepage": "https://github.com/Benjamin8585/base256#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/jest": "25.1.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "del-cli": "3.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "25.1.0",
    "prettier": "2.4.1",
    "ts-jest": "25.2.0",
    "typescript": "^4.7.4"
